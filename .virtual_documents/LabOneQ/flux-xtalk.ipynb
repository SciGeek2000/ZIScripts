def norm(results, thresh_low = -2, thresh_high = 2):
    angle_data = np.abs(np.unwrap(np.angle(results).T))
    norm = np.array(angle_data) - np.nanmedian(angle_data, axis=0)
    norm[(norm > thresh_high) | (norm < thresh_low)] = np.nan
    return norm


